@model DBA.Models.CarViewModel
<div class="container">
    <div class="row">
        <div class="col-sm-2">
            <img src="@Model.Image" width="190" />
        </div>
        <div class="col-sm-8">
            <span class="Caption">@Model.Caption</span>
            @Html.Raw(Model.Description)
        </div>
        <div class="col-sm-2">
            @Model.Price kr.
        </div>
    </div>
</div>
